<?xml version="1.0" encoding="UTF-8"?>
<project name="transport.mqttv5" default="maven" basedir=".">
	<!-- import of the global SSE Jenkins properties -->
	<property file="${user.home}/global-build.properties"/>
	<property file="${user.home}/qualimaster-build.properties"/>
	<include file="${user.home}/macros.xml"/>
	
    <!-- deploy to SSE Mvn repo -->
	<target name="maven"> 
	    <maven pomFile="pom.xml" goal="install"/>
		<mvnDeployResolveArtifact folderName="IIP-transport-spring-mqttv5" pomFile="pom.xml" jarFolder="target" artifactPattern="transport.spring*.*.*.jar" />
	</target>
</project>
